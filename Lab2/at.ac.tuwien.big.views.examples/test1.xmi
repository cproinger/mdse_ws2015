<?xml version="1.0" encoding="UTF-8"?>
<views:ViewModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:views="http://www.big.tuwien.ac.at/views"
    xsi:schemaLocation="http://www.big.tuwien.ac.at/views ../at.ac.tuwien.big.views/model/views.ecore">
  <viewGroups
      name="Institute"
      welcomeViewGroup="true">
    <views xsi:type="views:CreateView"
        name="CreateInstitute"
        header="Institute"
        description="This is a form for creating institutes."
        startView="true">
      <class
          href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.0"/>
      <layout/>
      <elementGroups
          header="Institute Details">
        <layout/>
        <viewElements
            xsi:type="views:Text"
            label="Number"
            elementID="CreateNumber"
            format="^[0-9]+$">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.0/@properties.0"/>
        </viewElements>
        <viewElements
            xsi:type="views:Text"
            label="Name"
            elementID="CreateName"
            format="^[a-zA-Z -]+$">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.0/@properties.1"/>
        </viewElements>
        <viewElements
            xsi:type="views:List"
            label="Professors"
            elementID="CreateProfessors">
          <link
              targetView="//@viewGroups.0/@views.2"
              label="Update"/>
          <link
              targetView="//@viewGroups.0/@views.1"
              label="Add"/>
          <association
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.6"/>
        </viewElements>
        <viewElements
            xsi:type="views:Table"
            label="Courses"
            elementID="CreateCourses">
          <link
              targetView="//@viewGroups.0/@views.4"
              label="Delete"/>
          <link
              targetView="//@viewGroups.0/@views.3"
              label="Add"/>
          <association
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.5"/>
          <columns
              label="Type"
              elementID="TypeColumn">
            <property
                href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1/@properties.0"/>
          </columns>
          <columns
              label="Title"
              elementID="TitleColumn">
            <property
                href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1/@properties.1"/>
          </columns>
          <columns
              label="Credits"
              elementID="CreditsColumn">
            <property
                href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1/@properties.2"/>
          </columns>
        </viewElements>
      </elementGroups>
    </views>
    <views xsi:type="views:CreateView"
        name="CreateProfessor"
        header="Professor">
      <class
          href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.3"/>
      <layout
          alignment="Horizontal"/>
      <elementGroups
          header="Personal Details">
        <layout/>
        <viewElements
            xsi:type="views:Text"
            label="First name"
            elementID="CreateFirstname">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.2/@properties.0"/>
        </viewElements>
        <viewElements
            xsi:type="views:Text"
            label="Last name"
            elementID="CreateLastname">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.2/@properties.1"/>
        </viewElements>
        <viewElements
            xsi:type="views:Text"
            label="E-Mail"
            elementID="CreateEmail"
            format="">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.2/@properties.2"/>
        </viewElements>
      </elementGroups>
      <elementGroups
          header="Status">
        <layout/>
        <viewElements
            xsi:type="views:Selection"
            label="Visiting professor"
            elementID="CreateVisitingProfessor">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.3/@properties.0"/>
          <selectionItems
              value="true"/>
          <selectionItems
              value="false"/>
        </viewElements>
        <viewElements
            xsi:type="views:Text"
            label="Home university"
            elementID="CreateHomeUniversity">
          <condition
              xsi:type="views:ComparisonCondition"
              conditionID="HideIfFalse"
              comparisonValue="false"
              property="//@viewGroups.0/@views.1/@elementGroups.1/@viewElements.0"/>
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.3/@properties.1"/>
        </viewElements>
      </elementGroups>
    </views>
    <views xsi:type="views:UpdateView"
        name="UpdateProfessor"
        header="Update Professor">
      <class
          href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.3"/>
      <layout/>
      <elementGroups
          header="Update E-Mail Address">
        <layout/>
        <viewElements
            xsi:type="views:Text"
            label="E-Mail"
            elementID="UpdateEmail"
            format="">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.2/@properties.2"/>
        </viewElements>
      </elementGroups>
    </views>
    <views xsi:type="views:CreateView"
        name="CreateCourse"
        header="Course">
      <class
          href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1"/>
      <layout/>
      <elementGroups
          header="Course Details">
        <layout/>
        <viewElements
            xsi:type="views:Selection"
            label="Type"
            elementID="CreateType">
          <condition
              xsi:type="views:CompositeCondition"
              type="Disable"
              conditionID="CourseTypeCondition"
              compositionType="Or">
            <composedConditions
                xsi:type="views:ComparisonCondition"
                type="Disable"
                conditionID="DisableIfLess"
                comparisonValue="1"
                property="//@viewGroups.0/@views.3/@elementGroups.0/@viewElements.2"
                comparisonType="Less"/>
            <composedConditions
                xsi:type="views:ComparisonCondition"
                type="Disable"
                conditionID="DisableIfGreater"
                comparisonValue="30"
                property="//@viewGroups.0/@views.3/@elementGroups.0/@viewElements.2"
                comparisonType="Greater"/>
          </condition>
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1/@properties.0"/>
          <selectionItems
              xsi:type="views:EnumerationLiteralItem"
              value="Lecture">
            <enumerationLiteral
                href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.4/@literals.0"/>
          </selectionItems>
          <selectionItems
              xsi:type="views:EnumerationLiteralItem"
              value="Thesis">
            <enumerationLiteral
                href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.4/@literals.1"/>
          </selectionItems>
        </viewElements>
        <viewElements
            xsi:type="views:Text"
            label="Title"
            elementID="CreateTitle"
            format="^[a-zA-Z -]+$">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1/@properties.1"/>
        </viewElements>
        <viewElements
            xsi:type="views:Text"
            label="Credits"
            elementID="CreateCredits"
            format="^[0-9]+.[0-9]$">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1/@properties.2"/>
        </viewElements>
        <viewElements
            xsi:type="views:Text"
            label="Description"
            elementID="CreateDescription"
            format=""
            long="true">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1/@properties.3"/>
        </viewElements>
      </elementGroups>
      <elementGroups
          header="First Meeting">
        <layout/>
        <viewElements
            xsi:type="views:DateTimePicker"
            label="Date"
            elementID="CreateDate"
            format="dddd, MMMM Do YYYY">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1/@properties.4"/>
        </viewElements>
        <viewElements
            xsi:type="views:DateTimePicker"
            label="Time"
            elementID="CreateTime"
            format="h:mm A">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1/@properties.5"/>
        </viewElements>
      </elementGroups>
      <elementGroups
          header="Lecture">
        <layout/>
        <condition
            xsi:type="views:ComparisonCondition"
            type="Show"
            conditionID="ShowIfLE"
            comparisonValue="LE"
            property="//@viewGroups.0/@views.3/@elementGroups.0/@viewElements.0"/>
        <viewElements
            xsi:type="views:Text"
            label="Topic"
            elementID="CreateTopic"
            format=""
            long="true">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1/@properties.6"/>
        </viewElements>
      </elementGroups>
      <elementGroups
          header="Thesis">
        <layout/>
        <condition
            xsi:type="views:ComparisonCondition"
            type="Show"
            conditionID="ShowIFTH"
            comparisonValue="TH"
            property="//@viewGroups.0/@views.3/@elementGroups.0/@viewElements.0"/>
        <viewElements
            xsi:type="views:Text"
            label="Keywords"
            elementID="CreateKeywords">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1/@properties.7"/>
        </viewElements>
      </elementGroups>
    </views>
    <views xsi:type="views:DeleteView"
        name="DeleteCourse"
        header="Course"
        description="Do you really want to delete this course?">
      <class
          href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1"/>
      <layout/>
      <elementGroups
          header="Deleted Course">
        <layout/>
        <viewElements
            xsi:type="views:Text"
            label="Title"
            elementID="DeleteTitle"
            format="">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1/@properties.1"/>
        </viewElements>
      </elementGroups>
    </views>
  </viewGroups>
  <viewGroups
      name="Courses">
    <views xsi:type="views:ClassIndexView"
        name="CourseIndex"
        header="Courses"
        description="List of all current courses."
        startView="true">
      <link
          targetView="//@viewGroups.1/@views.1"
          label="Show"/>
      <class
          href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1"/>
    </views>
    <views xsi:type="views:ReadView"
        name="ShowCourse"
        header="Course">
      <class
          href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1"/>
      <layout/>
      <elementGroups
          header="Course Details">
        <layout/>
        <viewElements
            xsi:type="views:Text"
            label="Type"
            elementID="ShowType">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1/@properties.0"/>
        </viewElements>
        <viewElements
            xsi:type="views:Text"
            label="Title"
            elementID="ShowTitle">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1/@properties.1"/>
        </viewElements>
        <viewElements
            xsi:type="views:Text"
            label="Description"
            elementID="ShowDescription"
            format=""
            long="true">
          <property
              href="../at.ac.tuwien.big.views/example/instituteDomainModel.xmi#//@domainModelElements.1/@properties.3"/>
        </viewElements>
      </elementGroups>
    </views>
  </viewGroups>
</views:ViewModel>
